generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model sitepreview {
  id          Int    @id
  url         String @unique(map: "url") @db.Char(20)
  title       String @db.Char(20)
  favicon     String @db.Char(20)
  description String @db.Char(20)
}
